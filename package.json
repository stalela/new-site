{
  "name": "stalela",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "dev:admin": "turbo dev --filter=admin",
    "dev:marketing": "turbo dev --filter=marketing-site",
    "build:admin": "turbo build --filter=admin",
    "build:marketing": "turbo build --filter=marketing-site"
  },
  "packageManager": "npm@9.8.1",
  "devDependencies": {
    "turbo": "^2"
  }
}
